  "keywords": [
    "library",
    "tool",
    "php"
  ],
  "homepage": "https://github.com/swoft-cloud/swoft-serialize",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "inhere",
      "email": "in.798@qq.com",
      "homepage": "http://www.yzone.net/"
    }
  ],
  "require": {
    "php": ">7.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
