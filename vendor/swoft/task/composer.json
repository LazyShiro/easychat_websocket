{
  "name": "swoft/task",
  "type": "library",
  "version": "v2.0.9",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "task",
    "routine"
  ],
  "description": "swoft task component",
  "license": "Apache-2.0",
  "require": {
    "swoft/framework": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Task\\": "src/"
    },
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Task\\Testing\\": "test/testing",
      "SwoftTest\\Task\\Unit\\": "test/unit"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "scripts": {
    "test": "php run.php -c phpunit.xml"
  }
}
